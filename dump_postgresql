    aquatoriumstates numeric(5,3) DEFAULT 35.3 NOT NULL,
    aerotoriastates numeric(5,3) DEFAULT 29.2 NOT NULL,
    continents_idcontinents integer NOT NULL,
    politicalregime_idpoliticalregime integer NOT NULL,
    formsofgovernment_idformsofgovernment integer NOT NULL,
    CONSTRAINT positive2 CHECK (((populationstates > (0)::numeric) AND (squarestates > (0)::numeric))),
    CONSTRAINT spaces CHECK (((drylandstates > (0)::numeric) AND (aquatoriumstates > (0)::numeric) AND (aerotoriastates > (0)::numeric)))
