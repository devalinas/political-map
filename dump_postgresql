--
-- TOC entry 225 (class 1259 OID 17506)
-- Name: list_states; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.list_states AS
 SELECT states.namestates AS "Назва держави",
    politicalregime.namepoliticalregime AS "Політичний режим",
    formsofgovernment.nameformsofgovernment AS "Форма правління",
    count(internationalorganizations_has_states.internationalorganizations_idinternationalorganizations) AS "Кількість організацій"
   FROM (((public.states
     JOIN public.politicalregime ON ((states.politicalregime_idpoliticalregime = politicalregime.idpoliticalregime)))
     JOIN public.formsofgovernment ON ((states.formsofgovernment_idformsofgovernment = formsofgovernment.idformsofgovernment)))
     JOIN public.internationalorganizations_has_states ON ((states.idstates = internationalorganizations_has_states.states_idstates)))
  GROUP BY states.namestates, politicalregime.namepoliticalregime, formsofgovernment.nameformsofgovernment
  ORDER BY states.namestates;


ALTER TABLE public.list_states OWNER TO postgres;

