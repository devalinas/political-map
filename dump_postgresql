--
-- TOC entry 218 (class 1259 OID 17411)
-- Name: regimes; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.regimes AS
 SELECT continents.namecontinents AS cont,
    politicalregime.namepoliticalregime AS polreg,
    count(politicalregime.idpoliticalregime) AS numbofreg
   FROM ((public.continents
     LEFT JOIN public.states ON ((continents.idcontinents = states.continents_idcontinents)))
     LEFT JOIN public.politicalregime ON ((states.politicalregime_idpoliticalregime = politicalregime.idpoliticalregime)))
  GROUP BY continents.namecontinents, politicalregime.namepoliticalregime
  ORDER BY continents.namecontinents, (count(politicalregime.idpoliticalregime)) DESC;


ALTER TABLE public.regimes OWNER TO postgres;

--
-- TOC entry 222 (class 1259 OID 17488)
-- Name: rezerv; Type: TABLE; Schema: public; Owner: postgres
--

CREATE TABLE public.rezerv (
    id_rez integer NOT NULL,
    rez_date_event timestamp without time zone,
    rez_event character varying(10) NOT NULL,
    rez_tm_event character varying(10) NOT NULL,
    rez_c_namecountries character varying(45) NOT NULL,
    rez_c_capital character varying(45) NOT NULL,
    rez_c_language character varying(30) NOT NULL,
