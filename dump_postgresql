CREATE TABLE public.continents (
    idcontinents integer NOT NULL,
    namecontinents character varying(45) NOT NULL,
    squarecontinents numeric(12,5) DEFAULT 485.963 NOT NULL,
    populationcontinents numeric(15,6) DEFAULT 83286.73 NOT NULL,
    averageheightcontinents numeric(5,2) DEFAULT 693.13 NOT NULL,
    largestsettlementcontinents character varying(50) NOT NULL,
    kolstates integer,
    kolcountries integer,
    CONSTRAINT positive CHECK (((squarecontinents > (0)::numeric) AND (populationcontinents > (0)::numeric) AND (averageheightcontinents > (0)::numeric)))
);


