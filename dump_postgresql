--
-- TOC entry 228 (class 1259 OID 17521)
-- Name: forms; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.forms AS
 SELECT formsofgovernment.nameformsofgovernment AS "Назва форми правління",
    formsofgovernment.typeformsofgovernment AS "Вид форми",
    formsofgovernment.meaningoftheform AS "Особливості"
   FROM public.formsofgovernment
  WHERE ((formsofgovernment.typeformsofgovernment)::text ~~ 'П%'::text)
  ORDER BY formsofgovernment.nameformsofgovernment;


