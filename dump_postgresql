--
-- TOC entry 224 (class 1259 OID 17501)
-- Name: comb; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.comb AS
 SELECT concat(states.namestates, ' (', states.populationstates, ' млн)') AS "Держава",
    concat(countries.namecountries, ' (', countries.populationcountries, ' млн)') AS "Країна"
   FROM ((public.states
     JOIN public.continents ON ((states.continents_idcontinents = continents.idcontinents)))
     JOIN public.countries ON ((countries.continents_idcontinents = continents.idcontinents)))
  WHERE ((states.populationstates + countries.populationcountries) > ALL ( SELECT avg((states_1.populationstates + countries_1.populationcountries)) AS avg
           FROM ((public.states states_1
             JOIN public.continents continents_1 ON ((states_1.continents_idcontinents = continents_1.idcontinents)))
             JOIN public.countries countries_1 ON ((countries_1.continents_idcontinents = continents_1.idcontinents)))))
  GROUP BY CUBE((concat(states.namestates, ' (', states.populationstates, ' млн)')), (concat(countries.namecountries, ' (', countries.populationcountries, ' млн)')));


ALTER TABLE public.comb OWNER TO postgres;

--
-- TOC entry 227 (class 1259 OID 17516)
-- Name: cont_states; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.cont_states AS
 SELECT states.namestates AS "Назва",
    states.capitalstates AS "Столиця",
    states.populationstates AS "Населення",
    states.nationallanguage AS "Державна мова",
    states.economictypologyofstates AS "Економічний тип",
    states.squarestates AS "Площа"
   FROM public.states
  WHERE (states.continents_idcontinents IN ( SELECT continents.idcontinents
           FROM public.continents
          WHERE (continents.populationcontinents <= (2000)::numeric)));


ALTER TABLE public.cont_states OWNER TO postgres;

