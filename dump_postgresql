--
-- TOC entry 226 (class 1259 OID 17511)
-- Name: int_org_release; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.int_org_release AS
 SELECT internationalorganizations.nameinternationalorganizations AS "Міжнародна організація",
    states.namestates AS "Держава",
    internationalorganizations_has_states.dateofentry AS "Дата вступу держави"
   FROM ((public.states
     JOIN public.internationalorganizations_has_states ON ((states.idstates = internationalorganizations_has_states.states_idstates)))
     JOIN public.internationalorganizations ON ((internationalorganizations_has_states.internationalorganizations_idinternationalorganizations = internationalorganizations.idinternationalorganizations)))
  WHERE (internationalorganizations.releasedate IS NOT NULL)
  ORDER BY internationalorganizations.nameinternationalorganizations;


ALTER TABLE public.int_org_release OWNER TO postgres;

