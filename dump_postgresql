--
-- TOC entry 229 (class 1259 OID 17525)
-- Name: states_int_org_80; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.states_int_org_80 AS
 SELECT internationalorganizations_has_states.states_idstates AS "Код держави",
    internationalorganizations_has_states.internationalorganizations_idinternationalorganizations AS "Код організації",
    internationalorganizations_has_states.dateofentry AS "Дата входу",
    internationalorganizations_has_states.releasedate AS "Дата виходу"
   FROM public.internationalorganizations_has_states
  WHERE (internationalorganizations_has_states.dateofentry <= '1980-09-12'::date);


ALTER TABLE public.states_int_org_80 OWNER TO postgres;

